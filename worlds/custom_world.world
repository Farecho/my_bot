<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <!-- Ground and sun -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- ROS 2 spawn plugin -->
    <plugin name="gazebo_ros_factory" filename="libgazebo_ros_factory.so"/>

  </world>
</sdf>
